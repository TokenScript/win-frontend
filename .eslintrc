{
  "parser": "@typescript-eslint/parser",
  "plugins": ["react", "@typescript-eslint", "spellcheck", "unused-imports"],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ],
    "unused-imports/no-unused-imports": "error",
    "unused-imports/no-unused-vars": [
      "warn",
      {
        "vars": "all",
        "varsIgnorePattern": "^_",
        "args": "after-used",
        "argsIgnorePattern": "^_"
      }
    ],
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off",
    "react/display-name": "off",
    "no-console": "warn",
    "spellcheck/spell-checker": [
      1,
      {
        "comments": false,
        "strings": true,
        "identifiers": true,
        "templates": true,
        "lang": "en_US",
        "skipWords": [
          "dict",
          "utils",
          "textarea",
          "replacer",
          "stringifies",
          "nullable",
          "dups",
          "overline",
          "dropdown",
          "grey",
          "checkbox",
          "webkit",
          "listbox",
          "breakpoint",
          "xsmall",
          "xlarge",
          "tooltip",
          "multiline",
          "nowrap",
          "textfield",
          "dapp",
          "perf",
          "href",
          "walletconnect",
          "iconify",
          "poller",
          "axios",
          "pathname",
          "firstname",
          "lastname",
          "birthdate",
          "noopener",
          "noreferrer",
          "windingtree",
          "mailto",
          "unhandledrejection",
          "ethers",
          "ethereum",
          "hexlify",
          "subheader",
          "calc",
          "bgcolor",
          "resize",
          "latlon",
          "urls"
        ],
        "skipIfMatch": ["http://[^s]*", "^[-\\w]+/[-\\w\\.]+$"],
        "minLength": 4
      }
    ]
  }
}
